{
  "name": "labeler",
  "version": "0.0.0",
  "private": true,
  "description": "A bot to automate issue labeling",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format:js": "eslint --fix src/**/*.ts",
    "format:non-js": "prettier --write \"./**/*.{md,yml,json}\"",
