    "test": "jest --verbose --colors"
  },
  "dependencies": {},
  "devDependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^3.0.0",
    "@octokit/types": "^5.0.1",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@zeit/ncc": "^0.20.5",
    "codecov": "^3.7.1",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "ts-jest": "^24.2.0",
    "typescript": "^3.9.5"
  }
